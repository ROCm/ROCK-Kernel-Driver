/dts-v1/;

#include "t8103.dtsi"
#include "t8103-jxxx.dtsi"

/ {
	compatible = "apple,j293", "apple,t8103", "apple,arm-platform";
	model = "Apple MacBook Pro (13-inch, M1, 2020)";
};

&bluetooth0 {
	brcm,board-type = "apple,honshu";
};

&wifi0 {
	brcm,board-type = "apple,honshu";
};

&i2c2 {
	status = "okay";
};

&i2c4 {
	status = "okay";
};
