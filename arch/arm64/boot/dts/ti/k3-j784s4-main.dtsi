		power-domains = <&k3_pds 140 TI_SCI_PD_EXCLUSIVE>;
		clocks = <&k3_clks 140 1>, <&k3_clks 140 2>;
		clock-names =  "clk_ahb", "clk_xin";
		assigned-clocks = <&k3_clks 140 2>;
		assigned-clock-parents = <&k3_clks 140 3>;
		bus-width = <8>;
		ti,otap-del-sel-legacy = <0x0>;
